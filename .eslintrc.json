{
  "extends": ["eslint:recommended", "plugin:react/recommended", "google"],
  "plugins": [
    "react"
  ],
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "off",
    "no-undef": "off",
    "react/prop-types": "off",
    "react/no-direct-mutation-state": "off",
    "require-jsdoc": "warn"
  }
}
