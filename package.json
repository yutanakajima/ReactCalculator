{
  "name": "calculator",
  "version": "1.0.0",
  "main": "src/js/main.js",
  "license": "MIT",
  "scripts": {
    "build": "run-s clean build:js build:css",
    "build:js": "webpack",
    "build:css": "postcss src/css/calculatorcss -d assets/css",
    "clean": "rm -f assets/js/*.js assets/css/*.css",
    "watch": "npm-run-all -p watch:*",
    "watch:js": "webpack --progress --colors --watch",
    "watch:css": "postcss src/css/calculator.css -d assets/css -w"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "add": "^2.0.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.12",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.1.1",
    "postcss-color-function": "^4.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-mixins": "^6.1.1",
    "postcss-nested": "^2.1.2",
    "postcss-simple-vars": "^4.1.0",
    "webpack": "^3.6.0",
    "yarn": "^1.1.0"
  }
}
